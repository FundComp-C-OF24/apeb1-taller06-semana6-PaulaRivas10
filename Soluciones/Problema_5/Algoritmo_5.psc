Algoritmo Problema_5
	Definir estudiante Como Caracter
	Definir nota Como Entero
	i = 0
	Mientras (i<=4) Hacer
		escribir "Nombre del estudiante"
		leer estudiante
		Escribir "Nota"
		leer nota
		si (nota >= 7) Entonces
			Escribir "Estudiante aprobado"
		SiNo
			Escribir "Estudiante reprobado"
		FinSi
		i = i+1
	FinMientras
	
	
	
FinAlgoritmo
